# MorphV2 Backend Environment Variables
# Copy this file to .env and fill in your values

# Application Settings
DEBUG=false

# OpenRouter API (unified provider - RECOMMENDED)
OPENROUTER_API_KEY=sk-or-v1-your-key-here

# OpenRouter Models
ARCHITECT_MODEL=anthropic/claude-sonnet-4
VISION_MODEL=openai/gpt-4o
VERIFIER_MODEL=anthropic/claude-3-haiku

# Default AI Provider: "openrouter" (recommended), "anthropic", or "openai"
DEFAULT_AI_PROVIDER=openrouter

# Legacy API Keys (fallback if not using OpenRouter)
ANTHROPIC_API_KEY=sk-ant-your-key-here
OPENAI_API_KEY=sk-your-key-here

# Legacy Model Configuration
ANTHROPIC_MODEL=claude-3-5-sonnet-20241022
OPENAI_MODEL=gpt-4o

# Generation Settings
MAX_ITERATIONS=5
DEFAULT_CANVAS_WIDTH=1200
DEFAULT_CANVAS_HEIGHT=630

# Upstash Redis (for Celery queue)
UPSTASH_REDIS_URL=rediss://default:your-password@your-endpoint.upstash.io:6379

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...
SUPABASE_JWT_SECRET=your-jwt-secret-from-supabase-settings
SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...

# CORS (comma-separated origins)
CORS_ORIGINS=http://localhost:3000,http://127.0.0.1:3000

